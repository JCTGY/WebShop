INSERT INTO PRODCUT (ID, NAME, IMG_URL, CATEGORY, PRICE) values (NULL, 'Acer Aspire 5 Slim Laptop, 15.6 inches Full HD IPS Display', 'https://images-na.ssl-images-amazon.com/images/I/71vvXGmdKWL._AC_SL1500_.jpg', 'electronic', '458.94');


	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "id")
	private Long id;
	
	@Column(name = "name")
	@NotNull
	@NotEmpty
	private String name;
	
	@Column(name = "img_url")
	@URL
	private String imgUrl;
	
	@Column(name = "category")
	@NotNull
	@NotEmpty
	private String category;
	
	@Column(name = "price")
	@NotNull
	private double price;